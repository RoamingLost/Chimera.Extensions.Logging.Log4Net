{
  "version": "1.0.0-*",
  "description": "log4net logger provider for Microsoft.Extensions.Logging. Compatible with ASP.NET Core and .NET Core official release (dotnet).",
  "title": "Apache log4net logging extensions for .NET Core",
  "authors": [ "RoamingLost" ],
  "packOptions": {
    "tags": [ "logging", "log4net", "Microsoft.Extensions.Logging", "log", "netcore" ],
    "projectUrl": "https://github.com/RoamingLost/Chimera.Extensions.Logging.Log4Net",
    "licenseUrl": "https://github.com/RoamingLost/Chimera.Extensions.Logging.Log4Net/blob/master/LICENSE",
    "iconUrl": "https://raw.githubusercontent.com/RoamingLost/Chimera.Extensions.Logging.Log4Net/master/icon.png",
    "repository": {
      "type": "git",
      "url": "git://github.com/RoamingLost/Chimera.Extensions.Logging.Log4Net"
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "log4net": "2.0.7",
    "Microsoft.Extensions.Logging.Abstractions": "1.1.0"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Xml": ""
      }
    },
    "netstandard1.3": {
      "buildOptions": {
          "define": [ "NETCORE" ]
      }
    },
    "netstandard1.5": {
      "buildOptions": {
          "define": [ "NETCORE" ]
      }
    }
  }
}
