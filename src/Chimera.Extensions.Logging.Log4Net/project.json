{
  "version": "1.0.0-*",
  "description": "log4net provider for Microsoft.Extensions.Logging",
  "authors": [ "RoamingLost", "John Stanley" ],
  "packOptions": {
    "tags": [ "log4net", "Microsoft.Extensions.Logging", "log", "netcore" ],
    "projectUrl": "https://github.com/RoamingLost/Chimera.Extensions.Logging.Log4Net",
    "licenseUrl": "https://github.com/RoamingLost/Chimera.Extensions.Logging.Log4Net/blob/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "git://github.com/RoamingLost/Chimera.Extensions.Logging.Log4Net"
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "warningsAsErrors": true
  },
  "dependencies": {
    "log4net": "2.0.7",
    "Microsoft.Extensions.Logging.Abstractions": "1.1.0"
  },
  "frameworks": {
    "net451": {
    },
    "netstandard1.3": {
      "buildOptions": {
          "define": [ "NETCORE" ]
      }
    },
    "netstandard1.5": {
      "buildOptions": {
          "define": [ "NETCORE" ]
      }
    }
  }
